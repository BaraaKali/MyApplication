<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.myapplication">

    <uses-permission android:name="android.permission.VIBRATE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">


        <activity
            android:name=".MyServiceNotDoneActivity"
            android:label="@string/title_activity_my_not_done_services"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".MyServiceDoneActivity"
            android:label="@string/title_activity_my_done_services"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".ShowServiceDone"
            android:label="@string/title_activity_my_done_services"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".MyServicesActivity"
            android:label="@string/title_activity_my_services"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".HelpActivity"
            android:label="@string/title_activity_help"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".MunicipalityInformation"
            android:label="@string/muni_information"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".RequestServiceActivity"
            android:label="@string/request_service" />
        <activity
            android:name=".SpecificSreviceActivity"
            android:label="@string/title_activity_all_services"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".AllServices"
            android:label="@string/title_activity_all_services"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".MyInformation"
            android:label="@string/title_activity_my_information"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".SideBarActivity"
            android:label="@string/title_activity_side_bar"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".HomeActivity"
            android:label="@string/title_activity_home"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".SignUpActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>